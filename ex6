import java.util.Scanner;

public class Vid6 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Nhập số nguyên N: ");
        int n = sc.nextInt();

        int csCuoi = Math.abs(n % 10);
        System.out.println("Chữ số cuối cùng: " + csCuoi);
    }
}
